{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": ["google", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "warn",
        "eqeqeq": "warn",
        "valid-jsdoc": "off",
        "linebreak-style": "off",
        "no-octal": "off",
        "no-octal-escape": "off",
        "no-throw-literal": "off",
        "max-len": 0
    }
}
